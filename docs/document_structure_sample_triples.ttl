@prefix : <http://proethica.org/ontology/intermediate#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@base <http://proethica.org/ontology/intermediate> .

# Sample Document Structure Triples for Testing

# Sample case document
:case23_4_document rdf:type owl:NamedIndividual ;
    rdf:type :DocumentElement ;
    :hasTextContent "Case 23-4: Acknowledging Errors in Design" ;
    rdfs:label "NSPE Case 23-4 Document" .

# Document metadata
:case23_4_number rdf:type owl:NamedIndividual ;
    rdf:type :CaseNumber ;
    :hasTextContent "23-4" ;
    :isPartOf :case23_4_document .

:case23_4_year rdf:type owl:NamedIndividual ;
    rdf:type :CaseYear ;
    :hasTextContent "2023" ;
    :isPartOf :case23_4_document .

:case23_4_title rdf:type owl:NamedIndividual ;
    rdf:type :CaseTitle ;
    :hasTextContent "Acknowledging Errors in Design" ;
    :isPartOf :case23_4_document .

# Document sections
:case23_4_facts rdf:type owl:NamedIndividual ;
    rdf:type :FactsSection ;
    :hasTextContent "Engineer T is employed as an engineering expert in matters regarding highway design and construction. During a recent trial regarding an accident allegedly caused by a defective guardrail, Engineer T was retained by the defendant, the State Highway Department..." ;
    :isPartOf :case23_4_document ;
    :precedesInDocument :case23_4_questions .

:case23_4_questions rdf:type owl:NamedIndividual ;
    rdf:type :QuestionsSection ;
    :isPartOf :case23_4_document ;
    :precedesInDocument :case23_4_references ;
    :followsInDocument :case23_4_facts .

:case23_4_references rdf:type owl:NamedIndividual ;
    rdf:type :ReferencesSection ;
    :isPartOf :case23_4_document ;
    :precedesInDocument :case23_4_discussion ;
    :followsInDocument :case23_4_questions .

:case23_4_discussion rdf:type owl:NamedIndividual ;
    rdf:type :DiscussionSection ;
    :isPartOf :case23_4_document ;
    :precedesInDocument :case23_4_conclusion ;
    :followsInDocument :case23_4_references .

:case23_4_conclusion rdf:type owl:NamedIndividual ;
    rdf:type :ConclusionSection ;
    :isPartOf :case23_4_document ;
    :followsInDocument :case23_4_discussion .

# Question items
:case23_4_question1 rdf:type owl:NamedIndividual ;
    rdf:type :QuestionItem ;
    :hasTextContent "Was it ethical for Engineer T and Engineer B to conclude an error had not been made in design?" ;
    :isPartOf :case23_4_questions ;
    :posesEthicalQuestion :ethicalDesignQuestion .

:case23_4_question2 rdf:type owl:NamedIndividual ;
    rdf:type :QuestionItem ;
    :hasTextContent "Was it ethical for Engineer T not to acknowledge an error after the accident occurred?" ;
    :isPartOf :case23_4_questions .

:case23_4_question3 rdf:type owl:NamedIndividual ;
    rdf:type :QuestionItem ;
    :hasTextContent "Was it ethical for Engineer T not to acknowledge an error during the deposition?" ;
    :isPartOf :case23_4_questions .

# Conclusion items
:case23_4_conclusion1 rdf:type owl:NamedIndividual ;
    rdf:type :ConclusionItem ;
    :hasTextContent "It was ethical for Engineer T and Engineer B to conclude no error had been made in design." ;
    :isPartOf :case23_4_conclusion ;
    :representsEthicalDecision :ethicalDecision1 .

:case23_4_conclusion2 rdf:type owl:NamedIndividual ;
    rdf:type :ConclusionItem ;
    :hasTextContent "It was ethical for Engineer T not to acknowledge an error after the accident occurred." ;
    :isPartOf :case23_4_conclusion ;
    :representsEthicalDecision :ethicalDecision2 .

:case23_4_conclusion3 rdf:type owl:NamedIndividual ;
    rdf:type :ConclusionItem ;
    :hasTextContent "It was ethical for Engineer T to refrain from acknowledging an error during the deposition." ;
    :isPartOf :case23_4_conclusion ;
    :representsEthicalDecision :ethicalDecision3 .

# Code references
:case23_4_code_ref1 rdf:type owl:NamedIndividual ;
    rdf:type :CodeReferenceItem ;
    :hasTextContent "Code of Ethics Section II.1.a - Engineers shall hold paramount the safety, health, and welfare of the public" ;
    :isPartOf :case23_4_references .

:case23_4_code_ref2 rdf:type owl:NamedIndividual ;
    rdf:type :CodeReferenceItem ;
    :hasTextContent "Code of Ethics Section III.1.b - Engineers shall advise their clients or employers when they believe a project will not be successful" ;
    :isPartOf :case23_4_references .

# Ethical context linking
:case23_4_context rdf:type owl:NamedIndividual ;
    rdf:type :EthicalContext ;
    rdfs:label "Engineering Expert Testimony Context" .

:case23_4_document :documentDescribesCase :case23_4_context .
:case23_4_facts :factsDescribeContext :case23_4_context .

# Agents mentioned in the document
:engineer_T rdf:type owl:NamedIndividual ;
    rdf:type :HumanAgent ;
    rdfs:label "Engineer T" .

:engineer_B rdf:type owl:NamedIndividual ;
    rdf:type :HumanAgent ;
    rdfs:label "Engineer B" .

:state_highway_department rdf:type owl:NamedIndividual ;
    rdf:type :OrganizationalAgent ;
    rdfs:label "State Highway Department" .

:case23_4_facts :mentionsAgent :engineer_T, :engineer_B, :state_highway_department .

# Linking to ethical principles
:honesty_principle rdf:type owl:NamedIndividual ;
    rdf:type :EthicalPrinciple ;
    rdfs:label "Honesty" .

:public_safety_principle rdf:type owl:NamedIndividual ;
    rdf:type :EthicalPrinciple ;
    rdfs:label "Public Safety" .

:case23_4_discussion :concernsPrinciple :honesty_principle, :public_safety_principle .

# Sample embedding
:case23_4_facts :hasEmbedding "[0.123, 0.456, 0.789, ...]"^^rdf:List .
