<http://proethica.org/guidelines/public_safety_and_welfare>
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/EthicalConcept>;
    <http://www.w3.org/2000/01/rdf-schema#label> """Public Safety and Welfare""";
    <http://purl.org/dc/elements/1.1/description> """The paramount obligation of engineers to prioritize the safety, health, and welfare of the public above all other considerations in their professional work.""";
    <http://proethica.org/ontology/hasCategory> """principle""";
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/Principle>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/public_safety_and_welfare/reference/1>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/public_safety_and_welfare/reference/2>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/public_safety_and_welfare/reference/3>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/public_safety_and_welfare/reference/4>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/risk_assessment>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/safety_standards>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/reporting_dangers>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/professional_responsibility>.

<http://proethica.org/guidelines/public_safety_and_welfare/reference/1>
    <http://proethica.org/ontology/referenceText> """Engineers must hold paramount the safety, health, and welfare of the public in the performance of their professional duties. This takes precedence over other considerations.""".

<http://proethica.org/guidelines/public_safety_and_welfare/reference/2>
    <http://proethica.org/ontology/referenceText> """Always conduct thorough risk assessments""".

<http://proethica.org/guidelines/public_safety_and_welfare/reference/3>
    <http://proethica.org/ontology/referenceText> """Never compromise safety standards for cost or schedule""".

<http://proethica.org/guidelines/public_safety_and_welfare/reference/4>
    <http://proethica.org/ontology/referenceText> """Promptly report potential dangers to appropriate authorities""".

<http://proethica.org/guidelines/professional_competence>
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/EthicalConcept>;
    <http://www.w3.org/2000/01/rdf-schema#label> """Professional Competence""";
    <http://purl.org/dc/elements/1.1/description> """The obligation for engineers to only perform services in areas where they have adequate expertise, knowledge, and skills.""";
    <http://proethica.org/ontology/hasCategory> """obligation""";
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/Obligation>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/professional_competence/reference/1>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/professional_competence/reference/2>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/professional_competence/reference/3>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/professional_competence/reference/4>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/continuous_education>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/technical_skills>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/professional_development>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/expert_engagement>.

<http://proethica.org/guidelines/professional_competence/reference/1>
    <http://proethica.org/ontology/referenceText> """Engineers shall perform services only in areas of their competence.""".

<http://proethica.org/guidelines/professional_competence/reference/2>
    <http://proethica.org/ontology/referenceText> """Maintain and improve technical skills through continuous education""".

<http://proethica.org/guidelines/professional_competence/reference/3>
    <http://proethica.org/ontology/referenceText> """Accept assignments only when qualified by education or experience""".

<http://proethica.org/guidelines/professional_competence/reference/4>
    <http://proethica.org/ontology/referenceText> """Engage qualified experts when projects require knowledge beyond personal expertise""".

<http://proethica.org/guidelines/truthfulness_in_public_statements>
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/EthicalConcept>;
    <http://www.w3.org/2000/01/rdf-schema#label> """Truthfulness in Public Statements""";
    <http://purl.org/dc/elements/1.1/description> """The requirement for engineers to be objective, truthful, and comprehensive in all public communications and professional reports.""";
    <http://proethica.org/ontology/hasCategory> """principle""";
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/Principle>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/truthfulness_in_public_statements/reference/1>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/truthfulness_in_public_statements/reference/2>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/truthfulness_in_public_statements/reference/3>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/truthfulness_in_public_statements/reference/4>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/transparency>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/disclosure>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/honesty>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/professional_communication>.

<http://proethica.org/guidelines/truthfulness_in_public_statements/reference/1>
    <http://proethica.org/ontology/referenceText> """Engineers shall issue public statements only in an objective and truthful manner.""".

<http://proethica.org/guidelines/truthfulness_in_public_statements/reference/2>
    <http://proethica.org/ontology/referenceText> """Disclose all pertinent information in professional reports, statements, and testimony""".

<http://proethica.org/guidelines/truthfulness_in_public_statements/reference/3>
    <http://proethica.org/ontology/referenceText> """Include all relevant data in public statements""".

<http://proethica.org/guidelines/truthfulness_in_public_statements/reference/4>
    <http://proethica.org/ontology/referenceText> """Avoid statements containing material misrepresentation of fact""".

<http://proethica.org/guidelines/faithful_agency>
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/EthicalConcept>;
    <http://www.w3.org/2000/01/rdf-schema#label> """Faithful Agency""";
    <http://purl.org/dc/elements/1.1/description> """The duty of engineers to act as faithful agents or trustees for their employers and clients, protecting their interests while maintaining ethical standards.""";
    <http://proethica.org/ontology/hasCategory> """obligation""";
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/Obligation>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/faithful_agency/reference/1>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/faithful_agency/reference/2>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/faithful_agency/reference/3>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/faithful_agency/reference/4>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/conflict_of_interest>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/confidentiality>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/professional_judgment>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/client_relationship>.

<http://proethica.org/guidelines/faithful_agency/reference/1>
    <http://proethica.org/ontology/referenceText> """Engineers shall act as faithful agents or trustees for each employer or client.""".

<http://proethica.org/guidelines/faithful_agency/reference/2>
    <http://proethica.org/ontology/referenceText> """Avoid conflicts of interest""".

<http://proethica.org/guidelines/faithful_agency/reference/3>
    <http://proethica.org/ontology/referenceText> """Protect confidential information""".

<http://proethica.org/guidelines/faithful_agency/reference/4>
    <http://proethica.org/ontology/referenceText> """Make employers or clients aware of consequences if professional judgment is overruled""".

<http://proethica.org/guidelines/professional_integrity>
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/EthicalConcept>;
    <http://www.w3.org/2000/01/rdf-schema#label> """Professional Integrity""";
    <http://purl.org/dc/elements/1.1/description> """The commitment to avoid deceptive acts in professional practice and to maintain honest representation of qualifications and capabilities.""";
    <http://proethica.org/ontology/hasCategory> """principle""";
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/Principle>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/professional_integrity/reference/1>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/professional_integrity/reference/2>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/professional_integrity/reference/3>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/professional_integrity/reference/4>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/honesty>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/ethical_conduct>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/professional_reputation>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/transparency>.

<http://proethica.org/guidelines/professional_integrity/reference/1>
    <http://proethica.org/ontology/referenceText> """Engineers shall avoid deceptive acts in the solicitation of professional employment.""".

<http://proethica.org/guidelines/professional_integrity/reference/2>
    <http://proethica.org/ontology/referenceText> """Present qualifications and capabilities honestly""".

<http://proethica.org/guidelines/professional_integrity/reference/3>
    <http://proethica.org/ontology/referenceText> """Avoid improper solicitation of professional employment""".

<http://proethica.org/guidelines/professional_integrity/reference/4>
    <http://proethica.org/ontology/referenceText> """Base professional reputation on merit of services""".

<http://proethica.org/guidelines/ethical_conduct>
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/EthicalConcept>;
    <http://www.w3.org/2000/01/rdf-schema#label> """Ethical Conduct""";
    <http://purl.org/dc/elements/1.1/description> """The overarching obligation for engineers to conduct themselves honorably, responsibly, ethically, and lawfully in all professional activities.""";
    <http://proethica.org/ontology/hasCategory> """principle""";
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/Principle>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/ethical_conduct/reference/1>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/ethical_conduct/reference/2>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/ethical_conduct/reference/3>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/ethical_conduct/reference/4>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/integrity>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/responsibility>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/legal_compliance>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/anticorruption>.

<http://proethica.org/guidelines/ethical_conduct/reference/1>
    <http://proethica.org/ontology/referenceText> """Engineers shall conduct themselves honorably, responsibly, ethically, and lawfully.""".

<http://proethica.org/guidelines/ethical_conduct/reference/2>
    <http://proethica.org/ontology/referenceText> """Maintain high standards of honesty and integrity""".

<http://proethica.org/guidelines/ethical_conduct/reference/3>
    <http://proethica.org/ontology/referenceText> """Zero tolerance for bribery, fraud, and corruption""".

<http://proethica.org/guidelines/ethical_conduct/reference/4>
    <http://proethica.org/ontology/referenceText> """Support colleagues who uphold ethical standards""".

<http://proethica.org/guidelines/sustainability>
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/EthicalConcept>;
    <http://www.w3.org/2000/01/rdf-schema#label> """Sustainability""";
    <http://purl.org/dc/elements/1.1/description> """The responsibility of engineers to consider environmental impacts and resource efficiency in their designs and to adhere to principles of sustainable development.""";
    <http://proethica.org/ontology/hasCategory> """principle""";
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/Principle>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/sustainability/reference/1>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/sustainability/reference/2>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/sustainability/reference/3>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/sustainability/reference/4>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/environmental_impact>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/resource_efficiency>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/waste_reduction>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/design_longevity>.

<http://proethica.org/guidelines/sustainability/reference/1>
    <http://proethica.org/ontology/referenceText> """Engineers should strive to adhere to principles of sustainable development.""".

<http://proethica.org/guidelines/sustainability/reference/2>
    <http://proethica.org/ontology/referenceText> """Consider environmental impacts in design decisions""".

<http://proethica.org/guidelines/sustainability/reference/3>
    <http://proethica.org/ontology/referenceText> """Minimize resource consumption and waste generation""".

<http://proethica.org/guidelines/sustainability/reference/4>
    <http://proethica.org/ontology/referenceText> """Design for longevity, reusability, and recyclability""".

<http://proethica.org/guidelines/intellectual_property_respect>
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/EthicalConcept>;
    <http://www.w3.org/2000/01/rdf-schema#label> """Intellectual Property Respect""";
    <http://purl.org/dc/elements/1.1/description> """The obligation to respect and properly attribute intellectual property rights, including patents, copyrights, and innovations.""";
    <http://proethica.org/ontology/hasCategory> """obligation""";
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/Obligation>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/intellectual_property_respect/reference/1>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/intellectual_property_respect/reference/2>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/intellectual_property_respect/reference/3>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/intellectual_property_respect/reference/4>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/attribution>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/patents>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/copyrights>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/documentation>.

<http://proethica.org/guidelines/intellectual_property_respect/reference/1>
    <http://proethica.org/ontology/referenceText> """Engineers shall respect intellectual property rights.""".

<http://proethica.org/guidelines/intellectual_property_respect/reference/2>
    <http://proethica.org/ontology/referenceText> """Properly attribute ideas and innovations""".

<http://proethica.org/guidelines/intellectual_property_respect/reference/3>
    <http://proethica.org/ontology/referenceText> """Honor patents, copyrights, and other forms of intellectual property""".

<http://proethica.org/guidelines/intellectual_property_respect/reference/4>
    <http://proethica.org/ontology/referenceText> """Maintain proper documentation of design decisions""".

<http://proethica.org/guidelines/whistleblowing>
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/EthicalConcept>;
    <http://www.w3.org/2000/01/rdf-schema#label> """Whistleblowing""";
    <http://purl.org/dc/elements/1.1/description> """The ethical responsibility to report unethical behavior or practices that could endanger public safety, following appropriate reporting procedures.""";
    <http://proethica.org/ontology/hasCategory> """obligation""";
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/Obligation>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/whistleblowing/reference/1>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/whistleblowing/reference/2>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/whistleblowing/reference/3>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/whistleblowing/reference/4>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/public_safety>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/reporting_procedures>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/ethical_responsibility>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/documentation>.

<http://proethica.org/guidelines/whistleblowing/reference/1>
    <http://proethica.org/ontology/referenceText> """Engineers have an ethical responsibility to report unethical behavior or practices that could endanger public safety.""".

<http://proethica.org/guidelines/whistleblowing/reference/2>
    <http://proethica.org/ontology/referenceText> """Follow proper reporting procedures within organizations""".

<http://proethica.org/guidelines/whistleblowing/reference/3>
    <http://proethica.org/ontology/referenceText> """If internal reporting fails, consider appropriate external reporting""".

<http://proethica.org/guidelines/whistleblowing/reference/4>
    <http://proethica.org/ontology/referenceText> """Document concerns and actions taken""".

<http://proethica.org/guidelines/emerging_technology_ethics>
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/EthicalConcept>;
    <http://www.w3.org/2000/01/rdf-schema#label> """Emerging Technology Ethics""";
    <http://purl.org/dc/elements/1.1/description> """The responsibility to consider broader societal implications when working with emerging technologies, including potential misuse and impacts on privacy and security.""";
    <http://proethica.org/ontology/hasCategory> """obligation""";
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/Obligation>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/emerging_technology_ethics/reference/1>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/emerging_technology_ethics/reference/2>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/emerging_technology_ethics/reference/3>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/emerging_technology_ethics/reference/4>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/technology_impact_assessment>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/privacy>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/security>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/multidisciplinary_collaboration>.

<http://proethica.org/guidelines/emerging_technology_ethics/reference/1>
    <http://proethica.org/ontology/referenceText> """Engineers working with emerging technologies should consider broader societal implications.""".

<http://proethica.org/guidelines/emerging_technology_ethics/reference/2>
    <http://proethica.org/ontology/referenceText> """Evaluate potential misuse of technology""".

<http://proethica.org/guidelines/emerging_technology_ethics/reference/3>
    <http://proethica.org/ontology/referenceText> """Consider impacts on privacy, security, and social well-being""".

<http://proethica.org/guidelines/emerging_technology_ethics/reference/4>
    <http://proethica.org/ontology/referenceText> """Engage with multidisciplinary experts to understand implications""".

<http://proethica.org/guidelines/cultural_and_global_awareness>
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/EthicalConcept>;
    <http://www.w3.org/2000/01/rdf-schema#label> """Cultural and Global Awareness""";
    <http://purl.org/dc/elements/1.1/description> """The obligation to respect cultural differences and consider global impacts of engineering work, including environmental and social consequences.""";
    <http://proethica.org/ontology/hasCategory> """principle""";
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/Principle>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/cultural_and_global_awareness/reference/1>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/cultural_and_global_awareness/reference/2>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/cultural_and_global_awareness/reference/3>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/cultural_and_global_awareness/reference/4>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/cultural_respect>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/global_impact>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/fair_labor>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/social_responsibility>.

<http://proethica.org/guidelines/cultural_and_global_awareness/reference/1>
    <http://proethica.org/ontology/referenceText> """Engineers should be aware of cultural differences and global impacts of their work.""".

<http://proethica.org/guidelines/cultural_and_global_awareness/reference/2>
    <http://proethica.org/ontology/referenceText> """Respect local cultural practices and values""".

<http://proethica.org/guidelines/cultural_and_global_awareness/reference/3>
    <http://proethica.org/ontology/referenceText> """Consider global environmental and social impacts""".

<http://proethica.org/guidelines/cultural_and_global_awareness/reference/4>
    <http://proethica.org/ontology/referenceText> """Promote fair labor practices throughout supply chains""".

<http://proethica.org/guidelines/public>
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/EthicalConcept>;
    <http://www.w3.org/2000/01/rdf-schema#label> """Public""";
    <http://purl.org/dc/elements/1.1/description> """The general population whose safety, health, and welfare must be prioritized in engineering work.""";
    <http://proethica.org/ontology/hasCategory> """stakeholder""";
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/Stakeholder>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/public/reference/1>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/public_safety_and_welfare>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/risk_assessment>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/social_impact>.

<http://proethica.org/guidelines/public/reference/1>
    <http://proethica.org/ontology/referenceText> """Engineers must hold paramount the safety, health, and welfare of the public in the performance of their professional duties.""".

<http://proethica.org/guidelines/employers_and_clients>
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/EthicalConcept>;
    <http://www.w3.org/2000/01/rdf-schema#label> """Employers and Clients""";
    <http://purl.org/dc/elements/1.1/description> """Organizations or individuals who engage engineering services and for whom engineers must act as faithful agents or trustees.""";
    <http://proethica.org/ontology/hasCategory> """stakeholder""";
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/Stakeholder>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/employers_and_clients/reference/1>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/faithful_agency>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/confidentiality>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/professional_judgment>.

<http://proethica.org/guidelines/employers_and_clients/reference/1>
    <http://proethica.org/ontology/referenceText> """Engineers shall act as faithful agents or trustees for each employer or client.""".

<http://proethica.org/guidelines/colleagues>
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/EthicalConcept>;
    <http://www.w3.org/2000/01/rdf-schema#label> """Colleagues""";
    <http://purl.org/dc/elements/1.1/description> """Fellow engineering professionals with whom engineers interact and whom they should support in upholding ethical standards.""";
    <http://proethica.org/ontology/hasCategory> """stakeholder""";
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/Stakeholder>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/colleagues/reference/1>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/professional_community>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/ethical_conduct>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/support>.

<http://proethica.org/guidelines/colleagues/reference/1>
    <http://proethica.org/ontology/referenceText> """Support colleagues who uphold ethical standards""".

<http://proethica.org/guidelines/risk_assessment>
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/EthicalConcept>;
    <http://www.w3.org/2000/01/rdf-schema#label> """Risk Assessment""";
    <http://purl.org/dc/elements/1.1/description> """The systematic process of evaluating potential risks and hazards associated with engineering projects to ensure public safety.""";
    <http://proethica.org/ontology/hasCategory> """action""";
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/Action>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/risk_assessment/reference/1>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/public_safety_and_welfare>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/safety_standards>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/professional_responsibility>.

<http://proethica.org/guidelines/risk_assessment/reference/1>
    <http://proethica.org/ontology/referenceText> """Always conduct thorough risk assessments""".

<http://proethica.org/guidelines/continuous_education>
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/EthicalConcept>;
    <http://www.w3.org/2000/01/rdf-schema#label> """Continuous Education""";
    <http://purl.org/dc/elements/1.1/description> """The ongoing process of maintaining and improving technical skills and knowledge throughout an engineer's career.""";
    <http://proethica.org/ontology/hasCategory> """action""";
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/Action>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/continuous_education/reference/1>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/professional_competence>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/technical_skills>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/professional_development>.

<http://proethica.org/guidelines/continuous_education/reference/1>
    <http://proethica.org/ontology/referenceText> """Maintain and improve technical skills through continuous education""".

<http://proethica.org/guidelines/honesty>
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/EthicalConcept>;
    <http://www.w3.org/2000/01/rdf-schema#label> """Honesty""";
    <http://purl.org/dc/elements/1.1/description> """The fundamental value of truthfulness and integrity in all professional communications and actions.""";
    <http://proethica.org/ontology/hasCategory> """value""";
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/Value>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/honesty/reference/1>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/honesty/reference/2>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/integrity>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/truthfulness_in_public_statements>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/professional_integrity>.

<http://proethica.org/guidelines/honesty/reference/1>
    <http://proethica.org/ontology/referenceText> """Maintain high standards of honesty and integrity""".

<http://proethica.org/guidelines/honesty/reference/2>
    <http://proethica.org/ontology/referenceText> """Present qualifications and capabilities honestly""".

<http://proethica.org/guidelines/responsibility>
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/EthicalConcept>;
    <http://www.w3.org/2000/01/rdf-schema#label> """Responsibility""";
    <http://purl.org/dc/elements/1.1/description> """The value of being accountable for one's professional actions and decisions and their consequences.""";
    <http://proethica.org/ontology/hasCategory> """value""";
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/Value>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/responsibility/reference/1>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/ethical_conduct>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/professional_integrity>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/public_safety_and_welfare>.

<http://proethica.org/guidelines/responsibility/reference/1>
    <http://proethica.org/ontology/referenceText> """Engineers shall conduct themselves honorably, responsibly, ethically, and lawfully.""".

<http://proethica.org/guidelines/safety_standards>
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/EthicalConcept>;
    <http://www.w3.org/2000/01/rdf-schema#label> """Safety Standards""";
    <http://purl.org/dc/elements/1.1/description> """Established guidelines and requirements designed to ensure the safety of engineering projects and products.""";
    <http://proethica.org/ontology/hasCategory> """constraint""";
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/Constraint>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/safety_standards/reference/1>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/public_safety_and_welfare>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/risk_assessment>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/professional_responsibility>.

<http://proethica.org/guidelines/safety_standards/reference/1>
    <http://proethica.org/ontology/referenceText> """Never compromise safety standards for cost or schedule""".

<http://proethica.org/guidelines/conflict_of_interest>
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/EthicalConcept>;
    <http://www.w3.org/2000/01/rdf-schema#label> """Conflict of Interest""";
    <http://purl.org/dc/elements/1.1/description> """Situations where personal interests could compromise professional judgment or obligations to employers, clients, or the public.""";
    <http://proethica.org/ontology/hasCategory> """constraint""";
    <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://proethica.org/ontology/Constraint>;
    <http://proethica.org/ontology/hasReference> <http://proethica.org/guidelines/conflict_of_interest/reference/1>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/faithful_agency>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/professional_integrity>;
    <http://proethica.org/ontology/relatedTo> <http://proethica.org/guidelines/ethical_conduct>.

<http://proethica.org/guidelines/conflict_of_interest/reference/1>
    <http://proethica.org/ontology/referenceText> """Avoid conflicts of interest""".
