@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix proethica: <http://proethica.org/ontology/> .
@prefix guideline: <http://proethica.org/test/> .

<http://proethica.org/test/public-safety> 
    rdf:type proethica:Principle ;
    rdfs:label "Public Safety" ;
    dc:description "The paramount obligation of engineers to prioritize public safety" ;
    proethica:hasConceptId "0" ;
    proethica:hasCategory "principle" ;
    proethica:relatedTo <http://proethica.org/test/professional-responsibility> ;
    proethica:relatedTo <http://proethica.org/test/risk-management> ;
    proethica:hasTextReference "Engineers shall hold paramount the safety of the public" ;
    proethica:guides proethica:ProfessionalBehavior .

<http://proethica.org/test/professional-competence> 
    rdf:type proethica:Obligation ;
    rdfs:label "Professional Competence" ;
    dc:description "The obligation to only perform work within one's area of competence" ;
    proethica:hasConceptId "1" ;
    proethica:hasCategory "obligation" ;
    proethica:relatedTo <http://proethica.org/test/professional-development> ;
    proethica:relatedTo <http://proethica.org/test/technical-expertise> ;
    proethica:hasTextReference "Engineers shall perform services only in areas of their competence" ;
    proethica:requiredOf proethica:Professional .

<http://proethica.org/test/ethical-behavior> 
    rdf:type proethica:Principle ;
    rdfs:label "Ethical Behavior" ;
    dc:description "Conducting oneself with integrity and adhering to ethical principles" ;
    proethica:hasConceptId "2" ;
    proethica:hasCategory "principle" ;
    proethica:relatedTo <http://proethica.org/test/honesty> ;
    proethica:relatedTo <http://proethica.org/test/professional-responsibility> ;
    proethica:hasTextReference "Engineers shall act in professional matters as faithful agents" ;
    proethica:guides proethica:ProfessionalBehavior .

