@prefix : <http://proethica.org/ontology/nspe-case-89-7-1#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix ar: <http://proethica.org/ontology/agent-role#> .
@prefix eng: <http://proethica.org/ontology/engineering-ethics#> .
@base <http://proethica.org/ontology/nspe-case-89-7-1> .

<http://proethica.org/ontology/nspe-case-89-7-1> rdf:type owl:Ontology ;
    owl:imports <http://purl.obolibrary.org/obo/bfo.owl> ;
    owl:imports <http://proethica.org/ontology/agent-role> ;
    owl:imports <http://proethica.org/ontology/engineering-ethics> ;
    rdfs:label "NSPE Case 89-7-1 Ontology"@en ;
    rdfs:comment "Representation of NSPE BER Case 89-7-1: Duty to Report Safety Violations"@en ;
    dc:creator "ProEthica AI"@en ;
    dc:date "2025-04-02"^^xsd:date .

#################################################################
#    Case Specific Individuals
#################################################################

# Engineers and Clients
:EngineerA rdf:type owl:NamedIndividual ,
                    ar:Character ;
    rdfs:label "Engineer A"@en ;
    rdfs:comment "The structural engineer retained to investigate the building's integrity"@en .

:ClientX rdf:type owl:NamedIndividual ,
                  ar:Character ;
    rdfs:label "Client X"@en ;
    rdfs:comment "The building owner who hired Engineer A"@en .

# Physical Entities
:ApartmentBuilding rdf:type owl:NamedIndividual ,
                            eng:Building ;
    rdfs:label "Apartment Building"@en ;
    rdfs:comment "The 60-year-old occupied apartment building being investigated"@en ;
    eng:hasDeficiency :ElectricalSystemDeficiency ,
                    :MechanicalSystemDeficiency .

:StructuralReport rdf:type owl:NamedIndividual ,
                          eng:StructuralReport ;
    rdfs:label "Structural Report"@en ;
    rdfs:comment "The report written by Engineer A about the structural integrity of the building"@en .

:ElectricalSystemDeficiency rdf:type owl:NamedIndividual ,
                                     eng:ElectricalSystemDeficiency ;
    rdfs:label "Electrical System Deficiency"@en ;
    rdfs:comment "Code violations in the electrical system of the apartment building"@en .

:MechanicalSystemDeficiency rdf:type owl:NamedIndividual ,
                                     eng:MechanicalSystemDeficiency ;
    rdfs:label "Mechanical System Deficiency"@en ;
    rdfs:comment "Code violations in the mechanical system of the apartment building"@en .

# Events and Actions
:HiringEvent rdf:type owl:NamedIndividual ,
                      bfo:BFO_0000015 ; # process
    rdfs:label "Hiring Event"@en ;
    rdfs:comment "Client X hires Engineer A to investigate structural integrity"@en ;
    bfo:BFO_0000057 :ClientX , :EngineerA . # has participant

:ConfidentialityInstruction rdf:type owl:NamedIndividual ,
                                     bfo:BFO_0000015 ; # process
    rdfs:label "Confidentiality Instruction"@en ;
    rdfs:comment "Client X instructs Engineer A to keep the structural report confidential"@en ;
    bfo:BFO_0000057 :ClientX , :EngineerA . # has participant

:BuildingInspection rdf:type owl:NamedIndividual ,
                             eng:StructuralInspectionProcess ;
    rdfs:label "Building Inspection"@en ;
    rdfs:comment "Engineer A's inspection of the apartment building"@en ;
    bfo:BFO_0000057 :EngineerA , :ApartmentBuilding . # has participant

:DeficiencyDiscovery rdf:type owl:NamedIndividual ,
                              eng:HazardDiscoveryEvent ;
    rdfs:label "Deficiency Discovery"@en ;
    rdfs:comment "Engineer A discovers code violations in electrical and mechanical systems"@en ;
    bfo:BFO_0000057 :EngineerA , :ElectricalSystemDeficiency , :MechanicalSystemDeficiency . # has participant

:ClientNotification rdf:type owl:NamedIndividual ,
                            bfo:BFO_0000015 ; # process
    rdfs:label "Client Notification"@en ;
    rdfs:comment "Engineer A informs Client X of the deficiencies and safety concerns"@en ;
    bfo:BFO_0000057 :EngineerA , :ClientX , :ElectricalSystemDeficiency , :MechanicalSystemDeficiency . # has participant

:ReportWriting rdf:type owl:NamedIndividual ,
                        bfo:BFO_0000015 ; # process
    rdfs:label "Report Writing"@en ;
    rdfs:comment "Engineer A writes the structural report with brief mention of deficiencies"@en ;
    bfo:BFO_0000057 :EngineerA , :StructuralReport . # has participant

:NonDisclosureEvent rdf:type owl:NamedIndividual ,
                            eng:NonDisclosureEvent ;
    rdfs:label "Non-Disclosure Event"@en ;
    rdfs:comment "Engineer A's decision not to report safety violations to authorities"@en ;
    bfo:BFO_0000057 :EngineerA , :ElectricalSystemDeficiency , :MechanicalSystemDeficiency . # has participant

# Roles
:InspectionEngineerRoleInstance rdf:type owl:NamedIndividual ,
                                      eng:InspectionEngineerRole ;
    rdfs:label "Inspection Engineer Role Instance"@en ;
    rdfs:comment "The inspection engineer role held by Engineer A in this case"@en ;
    ar:isRoleOf :EngineerA .

:ClientRoleInstance rdf:type owl:NamedIndividual ,
                           ar:ClientRole ;
    rdfs:label "Client Role Instance"@en ;
    rdfs:comment "The client role held by Client X in this case"@en ;
    ar:isRoleOf :ClientX .

# Ethical Dilemma
:ConfidentialityVsSafetyDilemmaInstance rdf:type owl:NamedIndividual ,
                                               eng:ConfidentialityVsSafetyDilemma ;
    rdfs:label "Confidentiality vs Safety Dilemma Instance"@en ;
    rdfs:comment "The ethical dilemma of confidentiality obligation versus duty to protect public safety"@en ;
    eng:involvesPrinciple :PublicSafetyPrincipleInstance , :ConfidentialityPrincipleInstance .

:PublicSafetyPrincipleInstance rdf:type owl:NamedIndividual ,
                                      eng:PublicSafetyPrinciple ;
    rdfs:label "Public Safety Principle Instance"@en ;
    rdfs:comment "The principle that engineers must prioritize public safety (NSPE Code I.1)"@en .

:ConfidentialityPrincipleInstance rdf:type owl:NamedIndividual ,
                                        eng:ConfidentialityPrinciple ;
    rdfs:label "Confidentiality Principle Instance"@en ;
    rdfs:comment "The principle that engineers must not disclose confidential information (NSPE Code II.1.c)"@en .

# Ethical Decision
:EthicalDecisionInstance rdf:type owl:NamedIndividual ,
                                eng:PublicSafetyVsConfidentialityDecision ;
    rdfs:label "Ethical Decision Instance"@en ;
    rdfs:comment "The NSPE Board's decision that Engineer A acted unethically by not reporting safety violations"@en ;
    eng:overridesPrinciple :ConfidentialityPrincipleInstance ;
    eng:upholdsPrinciple :PublicSafetyPrincipleInstance .

#################################################################
#    Object Property Assertions
#################################################################

# Temporal relations among events
:HiringEvent bfo:BFO_0000062 :ConfidentialityInstruction . # preceded by
:ConfidentialityInstruction bfo:BFO_0000062 :BuildingInspection . # preceded by
:BuildingInspection bfo:BFO_0000062 :DeficiencyDiscovery . # preceded by
:DeficiencyDiscovery bfo:BFO_0000062 :ClientNotification . # preceded by
:ClientNotification bfo:BFO_0000062 :ReportWriting . # preceded by
:ReportWriting bfo:BFO_0000062 :NonDisclosureEvent . # preceded by

# Engineer has roles and obligations
:EngineerA ar:hasRole :InspectionEngineerRoleInstance ;
          ar:hasObligation :PublicSafetyObligationInstance , :ConfidentialityObligationInstance .

:PublicSafetyObligationInstance rdf:type owl:NamedIndividual ,
                                     ar:PublicSafetyObligation ;
    rdfs:label "Public Safety Obligation Instance"@en ;
    ar:isObligationOf :EngineerA .

:ConfidentialityObligationInstance rdf:type owl:NamedIndividual ,
                                       ar:ConfidentialityObligation ;
    rdfs:label "Confidentiality Obligation Instance"@en ;
    ar:isObligationOf :EngineerA .

# Process-role relations
:BuildingInspection ar:realizesRole :InspectionEngineerRoleInstance .
:NonDisclosureEvent eng:violatesPrinciple :PublicSafetyPrincipleInstance .
