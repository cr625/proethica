@prefix : <http://proethica.org/ontology/engineering-ethics#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix proeth: <http://proethica.org/ontology/intermediate#> .
@prefix nspe: <http://proethica.org/nspe/> .
@base <http://proethica.org/ontology/engineering-ethics> .

<http://proethica.org/ontology/engineering-ethics> rdf:type owl:Ontology ;
    owl:imports <http://purl.obolibrary.org/obo/bfo.owl> ;
    owl:imports <http://proethica.org/ontology/intermediate> ;
    rdfs:label "Engineering Ethics Ontology"@en ;
    rdfs:comment "Domain-specific ontology for engineering ethics based on the ProEthica intermediate ontology, enhanced with NSPE concepts"@en ;
    dc:creator "ProEthica AI"@en ;
    dc:date "2025-04-05"^^xsd:date .

#################################################################
#    Engineering Capabilities
#################################################################

# Define engineering capability classes
:EngineeringCapability rdf:type owl:Class ;
    rdfs:label "Engineering Capability"@en ;
    rdfs:comment "A capability possessed by engineering professionals"@en .

:StructuralAnalysisCapability rdf:type owl:Class ;
    rdfs:subClassOf :EngineeringCapability ;
    rdfs:label "Structural Analysis Capability"@en ;
    rdfs:comment "The capability to analyze structural systems"@en .

:StructuralDesignCapability rdf:type owl:Class ;
    rdfs:subClassOf :EngineeringCapability ;
    rdfs:label "Structural Design Capability"@en ;
    rdfs:comment "The capability to design structural systems"@en .

:ElectricalSystemsDesignCapability rdf:type owl:Class ;
    rdfs:subClassOf :EngineeringCapability ;
    rdfs:label "Electrical Systems Design Capability"@en ;
    rdfs:comment "The capability to design electrical systems"@en .

:MechanicalSystemsDesignCapability rdf:type owl:Class ;
    rdfs:subClassOf :EngineeringCapability ;
    rdfs:label "Mechanical Systems Design Capability"@en ;
    rdfs:comment "The capability to design mechanical systems"@en .

:EngineeringConsultationCapability rdf:type owl:Class ;
    rdfs:subClassOf :EngineeringCapability ;
    rdfs:label "Engineering Consultation Capability"@en ;
    rdfs:comment "The capability to provide expert engineering advice"@en .

:ProjectManagementCapability rdf:type owl:Class ;
    rdfs:subClassOf :EngineeringCapability ;
    rdfs:label "Project Management Capability"@en ;
    rdfs:comment "The capability to manage engineering projects"@en .

:RegulatoryComplianceCapability rdf:type owl:Class ;
    rdfs:subClassOf :EngineeringCapability ;
    rdfs:label "Regulatory Compliance Capability"@en ;
    rdfs:comment "The capability to ensure compliance with regulations"@en .

:TechnicalReportingCapability rdf:type owl:Class ;
    rdfs:subClassOf :EngineeringCapability ;
    rdfs:label "Technical Reporting Capability"@en ;
    rdfs:comment "The capability to prepare technical reports"@en .

:SafetyAssessmentCapability rdf:type owl:Class ;
    rdfs:subClassOf :EngineeringCapability ;
    rdfs:label "Safety Assessment Capability"@en ;
    rdfs:comment "The capability to assess safety risks in engineering contexts"@en .

#################################################################
#    Engineering Professional Roles
#################################################################

# Engineering Roles
:EngineeringRole rdf:type owl:Class ;
    rdfs:subClassOf proeth:Role ;
    rdfs:label "Engineering Role"@en ;
    rdfs:comment "A professional role in engineering"@en .

:StructuralEngineerRole rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:Role ;
    rdfs:subClassOf :EngineeringRole ;
    rdfs:label "Structural Engineer Role"@en ;
    rdfs:comment "The role of an engineer who analyzes and designs structural systems"@en ;
    proeth:hasCapability :StructuralAnalysisCapability ;
    proeth:hasCapability :StructuralDesignCapability ;
    proeth:hasCapability :TechnicalReportingCapability ;
    proeth:hasCapability :SafetyAssessmentCapability ;
    proeth:hasCapability :RegulatoryComplianceCapability .

:ElectricalEngineerRole rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:Role ;
    rdfs:subClassOf :EngineeringRole ;
    rdfs:label "Electrical Engineer Role"@en ;
    rdfs:comment "The role of an engineer who designs and develops electrical systems"@en ;
    proeth:hasCapability :ElectricalSystemsDesignCapability ;
    proeth:hasCapability :TechnicalReportingCapability ;
    proeth:hasCapability :SafetyAssessmentCapability ;
    proeth:hasCapability :RegulatoryComplianceCapability .

:MechanicalEngineerRole rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:Role ;
    rdfs:subClassOf :EngineeringRole ;
    rdfs:label "Mechanical Engineer Role"@en ;
    rdfs:comment "The role of an engineer who designs and develops mechanical systems"@en ;
    proeth:hasCapability :MechanicalSystemsDesignCapability ;
    proeth:hasCapability :TechnicalReportingCapability ;
    proeth:hasCapability :SafetyAssessmentCapability ;
    proeth:hasCapability :RegulatoryComplianceCapability .

:ConsultingEngineerRole rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:Role ;
    rdfs:subClassOf :EngineeringRole ;
    rdfs:label "Consulting Engineer Role"@en ;
    rdfs:comment "The role of an engineer who provides expert advice to clients"@en ;
    proeth:hasCapability :EngineeringConsultationCapability ;
    proeth:hasCapability :TechnicalReportingCapability ;
    proeth:hasCapability :SafetyAssessmentCapability ;
    proeth:hasCapability :RegulatoryComplianceCapability .

:ProjectEngineerRole rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:Role ;
    rdfs:subClassOf :EngineeringRole ;
    rdfs:label "Project Engineer Role"@en ;
    rdfs:comment "The role of an engineer who manages engineering projects"@en ;
    proeth:hasCapability :ProjectManagementCapability ;
    proeth:hasCapability :TechnicalReportingCapability ;
    proeth:hasCapability :SafetyAssessmentCapability ;
    proeth:hasCapability :RegulatoryComplianceCapability .

# Client and Stakeholder Roles
:ClientRole rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:Role ;
    rdfs:subClassOf proeth:Role ;
    rdfs:label "Client Role"@en ;
    rdfs:comment "The role of a person or organization that commissions engineering services"@en .

:RegulatoryOfficialRole rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:Role ;
    rdfs:subClassOf proeth:Role ;
    rdfs:label "Regulatory Official Role"@en ;
    rdfs:comment "The role of a government official responsible for enforcing regulations"@en ;
    proeth:hasCapability :RegulatoryComplianceCapability .

:PublicStakeholderRole rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:Role ;
    rdfs:subClassOf proeth:Role ;
    rdfs:label "Public Stakeholder Role"@en ;
    rdfs:comment "The role of a member of the public affected by engineering work"@en .

# Additional NSPE-specific roles from nspe-engineering-ethics.ttl
:InspectionEngineerRole rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:Role ;
    rdfs:subClassOf :EngineeringRole ;
    rdfs:label "Inspection Engineer Role"@en ;
    rdfs:comment "The role of an engineer performing inspection services"@en ;
    proeth:hasCapability :SafetyAssessmentCapability ;
    proeth:hasCapability :RegulatoryComplianceCapability ;
    proeth:hasCapability :TechnicalReportingCapability .

:ConfidentialConsultantRole rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:Role ;
    rdfs:subClassOf :ConsultingEngineerRole ;
    rdfs:label "Confidential Consultant Role"@en ;
    rdfs:comment "The role of an engineer bound by confidentiality agreements"@en .

:BuildingOfficialRole rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:Role ;
    rdfs:subClassOf :RegulatoryOfficialRole ;
    rdfs:label "Building Official Role"@en ;
    rdfs:comment "The role of an official who enforces building codes and regulations"@en .

#################################################################
#    Engineering Conditions and Dilemmas
#################################################################

:EngineeringCondition rdf:type owl:Class ;
    rdfs:subClassOf proeth:Condition ;
    rdfs:label "Engineering Condition"@en ;
    rdfs:comment "A condition relevant to engineering practice"@en .

:SafetyHazard rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:ConditionType ;
    rdfs:subClassOf :EngineeringCondition ;
    rdfs:label "Safety Hazard"@en ;
    rdfs:comment "A condition that poses risk to safety in an engineering context"@en .

:StructuralDeficiency rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:ConditionType ;
    rdfs:subClassOf :SafetyHazard ;
    rdfs:label "Structural Deficiency"@en ;
    rdfs:comment "A weakness or flaw in a structural system that may compromise safety"@en .

:CodeViolation rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:ConditionType ;
    rdfs:subClassOf :EngineeringCondition ;
    rdfs:label "Code Violation"@en ;
    rdfs:comment "A condition that violates building codes or engineering standards"@en .

:ConfidentialityAgreement rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:ConditionType ;
    rdfs:subClassOf :EngineeringCondition ;
    rdfs:label "Confidentiality Agreement"@en ;
    rdfs:comment "A legal requirement to maintain confidentiality of information"@en .

:EngineeringEthicalDilemma rdf:type owl:Class ;
    rdfs:subClassOf proeth:EthicalDilemma ;
    rdfs:label "Engineering Ethical Dilemma"@en ;
    rdfs:comment "A situation involving conflicting ethical principles in engineering practice"@en .

:ConfidentialityVsSafetyDilemma rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:ConditionType ;
    rdfs:subClassOf :EngineeringEthicalDilemma ;
    rdfs:label "Confidentiality vs Safety Dilemma"@en ;
    rdfs:comment "A dilemma where confidentiality obligations conflict with public safety obligations"@en .

:CompetenceVsClientWishesDilemma rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:ConditionType ;
    rdfs:subClassOf :EngineeringEthicalDilemma ;
    rdfs:label "Competence vs Client Wishes Dilemma"@en ;
    rdfs:comment "A dilemma where professional competence limitations conflict with client demands"@en .

:QualityVsBudgetDilemma rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:ConditionType ;
    rdfs:subClassOf :EngineeringEthicalDilemma ;
    rdfs:label "Quality vs Budget Dilemma"@en ;
    rdfs:comment "A dilemma where quality standards conflict with budget constraints"@en .

# Additional condition types from NSPE ontology
:EngineeringDeficiency rdf:type owl:Class ;
    rdfs:subClassOf :EngineeringCondition ;
    rdfs:label "Engineering Deficiency"@en ;
    rdfs:comment "A problem, defect, or inadequacy in an engineered structure or system"@en .

:BuildingSystemDeficiency rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:ConditionType ;
    rdfs:subClassOf :EngineeringDeficiency ;
    rdfs:label "Building System Deficiency"@en ;
    rdfs:comment "A deficiency in a building system that may pose safety hazards"@en .

:ElectricalSystemDeficiency rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:ConditionType ;
    rdfs:subClassOf :BuildingSystemDeficiency ;
    rdfs:label "Electrical System Deficiency"@en ;
    rdfs:comment "A deficiency in a building's electrical system that may violate codes or standards"@en .

:MechanicalSystemDeficiency rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:ConditionType ;
    rdfs:subClassOf :BuildingSystemDeficiency ;
    rdfs:label "Mechanical System Deficiency"@en ;
    rdfs:comment "A deficiency in a building's mechanical system that may violate codes or standards"@en .

:ConflictOfInterestCondition rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:ConditionType ;
    rdfs:subClassOf :EngineeringCondition ;
    rdfs:label "Conflict of Interest Condition"@en ;
    rdfs:comment "A condition where an engineer has competing interests that may compromise professional judgment"@en .

:DualRoleConflict rdf:type owl:Class ;
    rdf:type proeth:EntityType ;
    rdf:type proeth:ConditionType ;
    rdfs:subClassOf :ConflictOfInterestCondition ;
    rdfs:label "Dual Role Conflict"@en ;
    rdfs:comment "A conflict where an engineer serves in two different roles with potentially competing interests"@en .

#################################################################
