:OntologyDevelopmentActivity a prov:Activity ;
    rdfs:label "Engineering Ethics Ontology Development"@en ;
    prov:startedAtTime "2024-04-05"^^xsd:date ;
    prov:endedAtTime "2025-01-15"^^xsd:date ;
    prov:wasAssociatedWith :ProEthicaDevelopmentTeam ;
    prov:used :NSPECodeOfEthics ,
              :ISOStandards ,
              :ProfessionalEngineeringPractice ;
    rdfs:comment "Ontology developed through analysis of engineering ethics cases and professional standards" .

:ProEthicaDevelopmentTeam a prov:Agent ;
    rdfs:label "ProEthica Development Team"@en ;
    prov:actedOnBehalfOf "ProEthica Project" ;
    rdfs:comment "Collaborative effort including human experts and AI assistance" .