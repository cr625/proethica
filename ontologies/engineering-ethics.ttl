:QualityAssuranceAction a proeth:EntityType,
        proeth:Action,
        owl:Class ;
    rdfs:label "Quality Assurance Action"@en ;
    rdfs:comment "Activities to ensure engineering work meets required standards and specifications"@en ;
    dc:source "ISO 9001; Engineering quality management standards" ;
    rdfs:isDefinedBy <http://proethica.org/ontology/engineering-ethics> ;
