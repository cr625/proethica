@prefix : <http://proethica.org/ontology/nspe-case-89-7-1#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix ar: <http://proethica.org/ontology/agent-role#> .
@prefix eng: <http://proethica.org/ontology/engineering-ethics#> .
@prefix nspe: <http://proethica.org/ontology/nspe-code#> .
@base <http://proethica.org/ontology/nspe-case-89-7-1> .

<http://proethica.org/ontology/nspe-case-89-7-1> rdf:type owl:Ontology ;
    owl:imports <http://purl.obolibrary.org/obo/bfo.owl> ;
    owl:imports <http://proethica.org/ontology/agent-role> ;
    owl:imports <http://proethica.org/ontology/engineering-ethics> ;
    rdfs:label "NSPE Case 89-7-1 Ontology"@en ;
    rdfs:comment "Ontology for NSPE BER Case 89-7-1 involving confidentiality vs. public safety"@en ;
    dc:creator "ProEthica AI"@en ;
    dc:date "2025-04-02"^^xsd:date .

#################################################################
#    Classes for Case 89-7-1
#################################################################

### StructuralEngineer class that aligns with BFO role
:StructuralEngineer rdf:type owl:Class ;
    rdfs:subClassOf eng:StructuralEngineer ;
    rdfs:subClassOf ar:EngineeringProfessional ;
    rdfs:label "Structural Engineer (Case 89-7-1)"@en ;
    rdfs:comment "A structural engineer who inspects buildings and may discover safety issues"@en .

### Building Owner class
:BuildingOwner rdf:type owl:Class ;
    rdfs:subClassOf eng:Client ;
    rdfs:subClassOf ar:HumanAgent ;
    rdfs:label "Building Owner"@en ;
    rdfs:comment "Owner of a building who hires an engineer for inspection services"@en .

### ApartmentBuilding - independent continuant material entity
:ApartmentBuilding rdf:type owl:Class ;
    rdfs:subClassOf bfo:BFO_0000040 ; # material entity
    rdfs:label "Apartment Building"@en ;
    rdfs:comment "A multi-unit residential building"@en .

### Building system deficiencies
:BuildingSystemDeficiency rdf:type owl:Class ;
    rdfs:subClassOf eng:BuildingSystemDeficiency ;
    rdfs:label "Building System Deficiency (89-7-1)"@en ;
    rdfs:comment "A deficiency in a building system that may pose safety hazards"@en .

:ElectricalSystemDeficiency rdf:type owl:Class ;
    rdfs:subClassOf :BuildingSystemDeficiency ;
    rdfs:label "Electrical System Deficiency"@en ;
    rdfs:comment "A deficiency in a building's electrical system that violates codes and standards"@en .

:MechanicalSystemDeficiency rdf:type owl:Class ;
    rdfs:subClassOf :BuildingSystemDeficiency ;
    rdfs:label "Mechanical System Deficiency"@en ;
    rdfs:comment "A deficiency in a building's mechanical system that violates codes and standards"@en .

### Inspection Engineer Role
:InspectionEngineerRole rdf:type owl:Class ;
    rdfs:subClassOf ar:ProfessionalServiceProviderRole ;
    rdfs:label "Inspection Engineer Role"@en ;
    rdfs:comment "The role of an engineer performing inspection services"@en .

### Confidential Consultant Role
:ConfidentialConsultantRole rdf:type owl:Class ;
    rdfs:subClassOf ar:ProfessionalServiceProviderRole ;
    rdfs:label "Confidential Consultant Role"@en ;
    rdfs:comment "The role of an engineer bound by confidentiality agreements"@en .

### Ethical Obligations specific to this case
:ConfidentialityToClientObligation rdf:type owl:Class ;
    rdfs:subClassOf ar:EthicalObligation ;
    rdfs:subClassOf ar:ConfidentialityObligation ;
    rdfs:label "Confidentiality to Client Obligation"@en ;
    rdfs:comment "An ethical obligation to maintain the confidentiality of client information"@en .

:PublicSafetyProtectionObligation rdf:type owl:Class ;
    rdfs:subClassOf ar:EthicalObligation ;
    rdfs:subClassOf ar:PublicSafetyObligation ;
    rdfs:label "Public Safety Protection Obligation"@en ;
    rdfs:comment "An ethical obligation to protect public safety when safety hazards are discovered"@en .

### Ethical principles from NSPE code applied in this case
:PublicSafetyPrinciple rdf:type owl:Class ;
    rdfs:subClassOf ar:EthicalPrinciple ;
    rdfs:label "Public Safety Principle"@en ;
    rdfs:comment "The principle that engineers shall hold paramount the safety, health, and welfare of the public (NSPE Code I.1)"@en .

:ConfidentialityPrinciple rdf:type owl:Class ;
    rdfs:subClassOf ar:EthicalPrinciple ;
    rdfs:label "Confidentiality Principle"@en ;
    rdfs:comment "The principle that engineers shall not reveal facts, data, or information without prior consent of the client (NSPE Code II.1.c)"@en .

:SafeDocumentsPrinciple rdf:type owl:Class ;
    rdfs:subClassOf ar:EthicalPrinciple ;
    rdfs:label "Safe Documents Principle"@en ;
    rdfs:comment "The principle that engineers shall not complete, sign or seal plans that are not safe to public health and welfare (NSPE Code III.2.b)"@en .

### Reports and Documents
:StructuralReport rdf:type owl:Class ;
    rdfs:subClassOf eng:StructuralInspectionReport ;
    rdfs:label "Structural Report"@en ;
    rdfs:comment "A report documenting findings of a structural inspection"@en .

:ConfidentialAgreement rdf:type owl:Class ;
    rdfs:subClassOf eng:ConfidentialityAgreement ;
    rdfs:label "Confidential Agreement"@en ;
    rdfs:comment "An agreement specifying that engineering findings should remain confidential"@en .

### Processes and Events
:StructuralInspectionProcess rdf:type owl:Class ;
    rdfs:subClassOf bfo:BFO_0000015 ; # process
    rdfs:label "Structural Inspection Process"@en ;
    rdfs:comment "The process of inspecting a building's structural integrity"@en .

:HazardDiscoveryEvent rdf:type owl:Class ;
    rdfs:subClassOf eng:SafetyHazardDiscovery ;
    rdfs:label "Hazard Discovery Event"@en ;
    rdfs:comment "An event where a safety hazard is discovered during a professional inspection"@en .

:ConfidentialReportingEvent rdf:type owl:Class ;
    rdfs:subClassOf eng:ConfidentialReportDelivery ;
    rdfs:label "Confidential Reporting Event"@en ;
    rdfs:comment "An event where findings are reported under confidentiality constraints"@en .

:NonDisclosureToAuthoritiesEvent rdf:type owl:Class ;
    rdfs:subClassOf bfo:BFO_0000015 ; # process
    rdfs:label "Non-Disclosure to Authorities Event"@en ;
    rdfs:comment "An event where safety hazards are not disclosed to appropriate authorities"@en .

:EthicalConflictSituation rdf:type owl:Class ;
    rdfs:subClassOf eng:EthicalPrinciplesConflict ;
    rdfs:label "Ethical Conflict Situation"@en ;
    rdfs:comment "A situation where an engineer faces conflicting ethical principles or obligations"@en .

### Decision Types
:PublicSafetyVsConfidentialityDecision rdf:type owl:Class ;
    rdfs:subClassOf eng:ConfidentialityVsSafetyDecision ;
    rdfs:label "Public Safety vs Confidentiality Decision"@en ;
    rdfs:comment "A decision weighing public safety obligations against confidentiality obligations"@en .

#################################################################
#    Object Properties
#################################################################

:hasDeficiency rdf:type owl:ObjectProperty ;
    rdfs:domain :ApartmentBuilding ;
    rdfs:range :BuildingSystemDeficiency ;
    rdfs:label "has deficiency"@en ;
    rdfs:comment "Relates a building to deficiencies that it has"@en .

:governs rdf:type owl:ObjectProperty ;
    rdfs:domain ar:EthicalPrinciple ;
    rdfs:range ar:EthicalObligation ;
    rdfs:label "governs"@en ;
    rdfs:comment "Relates an ethical principle to the ethical obligation it governs"@en .

:takesActionIn rdf:type owl:ObjectProperty ;
    rdfs:domain ar:Agent ;
    rdfs:range bfo:BFO_0000015 ; # process
    rdfs:label "takes action in"@en ;
    rdfs:comment "Relates an agent to a process in which they participate"@en .

:conflictsWith rdf:type owl:ObjectProperty ;
    rdfs:domain ar:EthicalObligation ;
    rdfs:range ar:EthicalObligation ;
    rdfs:label "conflicts with"@en ;
    rdfs:comment "Relates an ethical obligation to another with which it conflicts"@en .
