[Unit]
Description=Gunicorn instance to serve proethica with agent orchestrator
After=network.target

[Service]
User=chris
Group=www-data
WorkingDirectory=/home/chris/proethica
Environment="PATH=/home/chris/proethica/venv/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
EnvironmentFile=/home/chris/proethica/.env
ExecStart=/home/chris/proethica/run_proethica_with_agents.sh
Restart=always

[Install]
WantedBy=multi-user.target
